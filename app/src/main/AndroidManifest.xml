<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.annabels_app">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/connect"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/connect"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".drink.DrinkRecycler" />
        <activity android:name=".converse.ConverseCardSingle" />
        <activity android:name=".converse.ConverseCard" />
        <activity android:name=".converse.LevelRecycler" />
        <activity android:name=".converse.CategoryRecycler" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>